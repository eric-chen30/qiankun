  "main": "index.js",
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:vue-main": "cd vue-main && npm i",
    "install:vue-sub1": "cd vue-sub1 && npm i",
    "install:vue-sub2": "cd vue-sub2 && npm i",
    "install:react-sub": "cd react-sub && npm i",
    "start": "npm-run-all --parallel start:*",
    "start:vue-main": "cd vue-main && npm start",
    "start:vue-sub1": "cd vue-sub1 && npm start",
    "start:vue-sub2": "cd vue-sub2 && npm start",
    "start:react-sub": "cd react-sub && npm start",
    "build": "npm-run-all build:*",
    "build:vue-main": "cd vue-main && npm run build",
    "build:vue-sub1": "cd vue-sub1 && npm run build",
    "build:vue-sub2": "cd vue-sub2 && npm run build",
    "build:react-sub": "cd react-sub && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
